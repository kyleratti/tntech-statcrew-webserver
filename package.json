{
  "name": "tntech-statcrew-webserver",
  "version": "1.0.0",
  "description": "Webserver to serve .xml files generated by statcrew for Tennessee Tech University's sports program",
  "main": "lib/index.js",
  "bin": "lib/index.js",
  "scripts": {
    "clean": "rimraf ./lib/*",
    "build": "npm run clean && tsc -p src",
    "watch": "npm run clean && tsc -p src --watch",
    "prepublish": "rimraf ./bin/*",
    "publish": "npm run build && pkg --out-path ./bin/ --targets win-x86,linux --no-bytecode --public --public-packages '*' .",
    "start-dev": "node lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyleratti/tntech-statcrew-webserver.git"
  },
  "author": "Kyle Ratti",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/kyleratti/tntech-statcrew-webserver/issues"
  },
  "homepage": "https://github.com/kyleratti/tntech-statcrew-webserver#readme",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "^13.7.7",
    "@types/socket.io-client": "^1.4.32",
    "http-status-codes": "^1.4.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "moment": "^2.24.0",
    "socket.io": "^2.3.0"
  }
}
